<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.stage.Stage?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root minHeight="600" minWidth="450" resizable="true" title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/help_icon.png" />
  </icons>
  <scene>
    <Scene>
      <stylesheets>
        <URL value="@HelpWindow.css" />
      </stylesheets>
      <VBox>
        <HBox alignment="CENTER" fx:id="helpMessageContainer">
          <children>
            <Label fx:id="helpMessage" text="Label">
              <HBox.margin>
                <Insets right="5.0" />
              </HBox.margin>
            </Label>
            <Button fx:id="copyButton" mnemonicParsing="false" onAction="#copyUrl" text="Copy URL">
              <HBox.margin>
                <Insets left="5.0" />
              </HBox.margin>
            </Button>
          </children>
          <opaqueInsets>
            <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
          </opaqueInsets>
          <padding>
            <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
          </padding>
        </HBox>
        <HBox fx:id="listCommandContainer">
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <children>
            <Label fx:id="listCommandDesc" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
            <Label fx:id="listCommand" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
        <HBox>
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <Label fx:id="listCommandExample" text="Label" style="-fx-font-weight: bold">
            <HBox.margin>
              <Insets right="10.0" />
            </HBox.margin>
          </Label>
        </HBox>
        <HBox fx:id="exitCommandContainer">
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <children>
            <Label fx:id="exitCommandDesc" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
            <Label fx:id="exitCommand" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
        <HBox>
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <Label fx:id="exitCommandExample" text="Label" style="-fx-font-weight: bold">
            <HBox.margin>
              <Insets right="10.0" />
            </HBox.margin>
          </Label>
        </HBox>
        <HBox fx:id="addCommandContainer">
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <children>
            <Label fx:id="addCommandDesc" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
            <Label fx:id="addCommand" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
        <HBox>
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <Label fx:id="addCommandExample" text="Label" style="-fx-font-weight: bold">
            <HBox.margin>
              <Insets right="10.0" />
            </HBox.margin>
          </Label>
        </HBox>
        <HBox fx:id="addGroupCommandContainer">
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <children>
            <Label fx:id="addGroupCommandDesc" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
            <Label fx:id="addGroupCommand" text="Label" style="-fx-font-weight: bold">
              <HBox.margin>
                <Insets right="10.0" />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
        <HBox>
          <padding>
            <Insets top="5" right="5" bottom="5" left="15" />
          </padding>
          <Label fx:id="addGroupCommandExample" text="Label" style="-fx-font-weight: bold">
            <HBox.margin>
              <Insets right="10.0" />
            </HBox.margin>
          </Label>
        </HBox>
      </VBox>
    </Scene>
  </scene>
</fx:root>
